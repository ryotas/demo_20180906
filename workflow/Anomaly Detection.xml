            <SQLExpression>"N$10015" as (SELECT  t0."CIF", 
 t0."AMOUNT_SUM_BY_MERCHANT", 
 t0."TX_ID_COUNT_BY_MERCHANT", 
 t1."TX_ID_COUNT", 
 t1."AMOUNT_SUM"
FROM
  (SELECT "CIF", 
	CAST(COLLECT(DM_NESTED_NUMERICAL(ID1, "AMOUNT_SUM_BY_MERCHANT")) AS DM_NESTED_NUMERICALS  )  "AMOUNT_SUM_BY_MERCHANT", 
	CAST(COLLECT(DM_NESTED_NUMERICAL(ID1, "TX_ID_COUNT_BY_MERCHANT")) AS DM_NESTED_NUMERICALS  )  "TX_ID_COUNT_BY_MERCHANT"
	FROM
		(SELECT   "CIF",
		("MERCHANT") ID1,
		SUM("AMOUNT") "AMOUNT_SUM_BY_MERCHANT",
		COUNT("TX_ID") "TX_ID_COUNT_BY_MERCHANT"
		FROM {INPUT_SOURCE}  
		GROUP BY "CIF", (("MERCHANT")) )
	 GROUP BY "CIF")  t0,
  (SELECT  "CIF", 
    COUNT("TX_ID") "TX_ID_COUNT"
    ,SUM("AMOUNT") "AMOUNT_SUM"
  FROM {INPUT_SOURCE}
   
  GROUP BY "CIF") t1
 WHERE  t0."CIF" =  t1."CIF"(+)
 )</SQLExpression>
            <AggregationElements>
